<div id="page" class="top_rule">
	<div id="content">
		<% cache :all_cities do %>
		<%= will_paginate @all_cities %>			
		<ul class="github_rank">
		  <% @all_cities.each_with_index do |city,index| %>
		    <li><%= determine_rank_for_paging(index) %>. <%= show_city_name(city.city) %> <%= "(#{pluralize(city.count,"coder")} #{number_with_delimiter(city.total,{:delimiter => ","})} points)" %></li>  
		  <% end %>
		</ul>
		<% end %>
	</div>
</div>