<div id="top_cities">
	<h3>Top Cities</h3>
	<ol>
		<% @top_ten_cities.each do |city| %>
		<li><%= show_city_name(city.city) %> <em><%= "(#{pluralize(city.count,"coder")} #{number_with_delimiter(city.total,{:delimiter => ","})} points)" %></em></li>
		<% end %>
	</ol>
	<%= link_to "View All Ranked Cities", all_cities_coders_path, :class => "rank_list" %>
</div>